page {
    width: 100%;
    height: 100%;
    background-color: #f0d6a1;
}

.container {
    width: 100%;
    height: auto;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    padding: 20rpx;
}

.title {
    font-size: 50rpx;
}

image {
    margin: 20rpx;
    width: 400rpx;
}

.description {
    margin: 20rpx;
    text-indent: 2rem;
}