page {
    width: 100%;
    height: 100%;
    background-color: #f0d6a1;
}

.container {
    width: 100%;
    height: 100%;
    padding: 30rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: flex-start;
    flex-wrap: wrap;
}

.item {
    width: 320rpx;
    display: flex;
    flex-direction: column;
    margin-bottom: 50rpx;
}

.man {
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    border: 1rpx solid #bb9563;
}

image {
    width: 100%;
}

.name {
    margin: 10rpx;
}

.hornor {
    width: 100%;
    margin-top: 10rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}

button {
    margin: 0;
    background-color: #732a1b;
    color: #f0d6a1;
}