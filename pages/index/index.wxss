page {
    width: 100%;
    height: 100%;
    background-color: #f0d6a1;
}

.container {
    padding: 0;
    width: 100%;
    height: 100%;
}

.content {
    position: absolute;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

swiper {
    width: 100%;
    height: 500rpx;
}

swiper-item {
    width: 100%;
}

image {
    width: 100%;
}

.title {
    margin-top: 80rpx;
    color: white;
    background-color: #a3691a;
    padding: 15rpx 30rpx;
    font-size: 30rpx;
}

.welcome {
    font-size: 25rpx;
    margin: 35rpx;
}

.row {
    width: 70%;
    margin-bottom: 40rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.column {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.function {
    color: #a3691a;
    margin: 10rpx;
}

.divLine {
    width: 180rpx;
    height: 1rpx;
    background: #a3691a;
}

dialog {
    width: 100%; 
    position: absolute;
}

.item {
    margin: 30rpx;
}